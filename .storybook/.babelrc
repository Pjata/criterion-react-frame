{
  "presets": [
    "env",
    "stage-0",
    "react"
  ],
  "plugins": [
    "babel-plugin-transform-export-extensions",
    "transform-es2015-modules-commonjs",
    "transform-regenerator",
    [
      "transform-runtime",
      {
        "helpers": true,
        "polyfill": true,
        "regenerator": true
      }
    ]
  ]
}
